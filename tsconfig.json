{
   "compilerOptions": {
      "target": "es5",
      "module": "es6",
      "outDir": "extensionJS",
      "sourceMap": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "lib": ["DOM", "ES5", "ES6", "ESNext"]
   },
   "include": ["src/**/*.ts"],
   "exclude": ["node_modules"]
}
